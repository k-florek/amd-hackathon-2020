          li.nav-item.dropdown.mx-0.mx-lg-1
              .mx-auto
                a.dropdown-toggle.nav-link.py-3.px-0.px-lg-3.rounded(href='#' data-toggle="dropdown" id="resourceMenu" aria-haspopup="true" aria-expanded="false") Resources
