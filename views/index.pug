// Sign up form
if registration_open
  #signup.portfolio-modal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='signupLabel' aria-hidden='true')
    .modal-dialog.modal-xl(role='document')
      .modal-content
        button.close(type='button' data-dismiss='modal' aria-label='Close')
          span(aria-hidden='true')
            i.fas.fa-times
        .modal-body.text-center
          .container
            .row.justify-content-center
              .col-lg-8
                // Sign Up - Title
                h2#signupLabel.portfolio-modal-title.text-secondary.text-uppercase.mb-0 Binfo Scavenger Hunt </br>Sign Up
                // Icon Divider
                .divider-custom
                  .divider-custom-line
                  .divider-custom-icon
                    i.fas.fa-dna
                  .divider-custom-line
                // Signup Form
                form.form-horizontal(id='signupForm' action='/signup')
                  .form-label-group
                    label(for='teamName')
                      h4 Team Name
                    input.form-control(name='teamName' type='text' placeholder='Team Name' required='' autofocus='')
                  br
                  .form-label-group
                    label(for='contactEmail') 
                      h4 Contact Email
                    input.form-control(name='contactEmail' type='email' placeholder='Email address' required='')
                  br
                  .form-label-group
                    label(for='teamMembers') 
                      h4 Team Members
                    ul
                      //textarea.form-control(name='teamMembers' type='text' placeholder='Jane Smith\nJohn Smith' required='')
                      li.list-group-item
                        input.form-control(name='teamMemberOne' type='text' placeholder='First' required='')
                      li.list-group-item
                        input.form-control(name='teamMemberTwo' type='text' placeholder='Second')
                      li.list-group-item
                        input.form-control(name='teamMemberThree' type='text' placeholder='Third')
                      li.list-group-item
                        input.form-control(name='teamMemberFour' type='text' placeholder='Fourth')
                      li.list-group-item
                        input.form-control(name='teamMemberFive' type='text' placeholder='Fith')
                  br
                  button.btn.btn-primary(name='signupsubmit' type='submit') Submit
                  br
                  .message.h5

